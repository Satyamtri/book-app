<div class="main-box">
  <%= render :partial => "layouts/sidebar" %>
  <div class="container mt-4">
    <div class="box shadow rounded">
      <table class="table table-hover table-striped table-bordered">
          <thead>
            <tr>
                <th>S.no</th> 
                <th>User_id</th> 
                <th>Created_at</th> 
                <th>Updated_at</th> 
                <th colspan='2'>State</th> 
            </tr>
          </thead>
        <tbody>
          <% @orders.each do |order| %>
              <tr>
                  <td><%= order.id %></td>
                  <td><%= order.user_id %></td>
                  <td><%= order.created_at%></td>
                  <td><%= order.updated_at%></td>
                  <td><%= order.state %></td>
                  
                  <td><%= link_to "" , order_path(order), class: "fa fa-trash btn btn-danger", method: :delete, data: {confirm: "are you sure!!"} %></td>
              </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <div class="flickr_pagination">
        <%= will_paginate @orders, :container => false %>
    </div>
  </div>
</div>
