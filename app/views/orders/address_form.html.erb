<div class="container" >

    <%= form_with scope: :address, url: addaddress_path, method: :post do |form| %>
    <div class="form-group mt-2">
        <%= form.label :FirstName %></br>
        <%= form.text_field :first_name %>
    </div>
    <div class="form-group mt-2">
        <%= form.label :LastName %></br>
        <%= form.text_field :last_name %>
    </div>
    <div class="form-group mt-2">
        <%= form.label :Contact %></br>
        <%= form.text_field :contactnumber %>
    </div>
    <div class="form-group mt-2">
        <%= form.label :Pincode %></br>
        <%= form.text_field :pincode %>
    </div>
    <div class="form-group mt-2">
        <%= form.label :City %></br>
        <%= form.text_field :city %>
    </div>
    <div class="form-group mt-2">
        <%= form.label :State %></br>
        <%= form.text_field :state %>
    </div>
    <div class="form-group mt-2">
        <%= form.label :Country %></br>
        <%= form.text_field :country %>
    </div>
    <div class="form-group mt-2">
        <%= form.label :landmark %></br>
        <%= form.text_field :landmark %>
    </div>
    <div class="form-group mt-2">
        <%= form.hidden_field :user_id, value: current_user.id  %>
    </div>
    <div class="form-group mt-2">
        <%= form.submit  %>
    </div>
    <% end %>
</div>